jupyter labextension install jupyterlab_templates
jupyter serverextension enable --py jupyterlab_templates --sys-prefix

jupyter lab build

jupyter notebook --generate-config -y 

git clone https://github.com/chekos/nb_templates.git /home/jovyan/.jupyter/nb_templates
echo "c.JupyterLabTemplates.template_dirs = ['/home/jovyan/.jupyter/nb_templates/nb_templates']" >> /home/jovyan/.jupyter/jupyter_notebook_config.py

rm /home/jovyan/conda-bld -rf
